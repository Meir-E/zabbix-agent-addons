                        </application>
                        <application>
                            <name>Web</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <params>$.stor_swap_used_percent</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>squid.check[all]</key>
                    </master_item>
                </item>
            </items>
            <macros>
                <macro>
                    <macro>{$SQUID_STATUS_URL}</macro>
                    <value>http://127.0.0.1:3128/squid-internal-mgr/info</value>
